    @Html.Partial("WashingCommonScript")
   
    <script type="text/javascript">
        $(function () {
            $(".select2").select2();
            $('#txtLeaveDateRange').daterangepicker();
        });
    </script>
    <script type="text/javascript">
        var hasBlankError = true;
        var hasMatchError = true;
        var pwd1, pwd2;
        $('#aChangePwd').on('click', function () {
            $('.pwd-change-msg').hide();
            $('.pwd-change-input').show();
            $('#changePwdModal').modal('show');
        });
        $("input[type=password]").keyup(function () {
            var ucase = new RegExp("[A-Z]+");
            var lcase = new RegExp("[a-z]+");
            var num = new RegExp("[0-9]+");
            pwd1 = $("#password1").val();
            pwd2 = $("#password2").val();
            if (pwd1 == "" || pwd2 == "") {
                $("#blank").removeClass("glyphicon-ok");
                $("#blank").addClass("glyphicon-remove");
                $("#blank").css("color", "#FF0004");
                hasBlankError = true;
            }
            else {
                $("#blank").removeClass("glyphicon-remove");
                $("#blank").addClass("glyphicon-ok");
                $("#blank").css("color", "#00A41E");
                hasBlankError = false;
            }

            if ($("#password1").val() == $("#password2").val()) {
                $("#pwmatch").removeClass("glyphicon-remove");
                $("#pwmatch").addClass("glyphicon-ok");
                $("#pwmatch").css("color", "#00A41E");
                hasMatchError = false;
            } else {
                $("#pwmatch").removeClass("glyphicon-ok");
                $("#pwmatch").addClass("glyphicon-remove");
                $("#pwmatch").css("color", "#FF0004");
                hasMatchError = true;
            }
        });
        $('#btnChangePwd').on('click', function () {
            if (hasBlankError == false && hasMatchError == false) {
                $.post(baseURL + '/LogIn/ChangePassword', { Password: pwd1, RepeatPassword: pwd2 }
            , function (response) {
                $('.pwd-change-msg h1').html(response.Data);
                $('.pwd-change-msg').show();
                $('.pwd-change-input').hide();
            });
            }
        });

        function checkNumeric(event) {
            var key = window.event ? event.keyCode : event.which;
            if (event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 46
             || event.keyCode == 37 || event.keyCode == 39) {
                return true;
            }
            if (event.which === 13) {
                $.next().focus();
            }
            else if (key < 48 || key > 57) {
                return false;
            }
            else return true;
        }
        $('.numericField').on('keydown', function (e) { -1 !== $.inArray(e.keyCode, [8, 9, 27, 13, 110, 190]) || /65|67|86|88/.test(e.keyCode) && (!0 === e.ctrlKey || !0 === e.metaKey) || 35 <= e.keyCode && 40 >= e.keyCode || (e.shiftKey || 48 > e.keyCode || 57 < e.keyCode) && (96 > e.keyCode || 105 < e.keyCode) && e.preventDefault() });
        // Input Validation Start
        $('form')
            //.on('blur', 'input[required], input.optional, select.required, .hidden-validation', validator.checkField)
            .on('change', 'select.required, .hidden-validation', validator.checkField)
            .on('keypress', 'input[required][pattern]', validator.keypress);

        $('form').on('blur', 'input[required],input.optional,select.required, .hidden-validation', function () {
            validator.checkField;
        });

        $(".checkSelect").blur(function () {
            validator.checkSelect($(this));
            $(this).focus();
        });

        $('.multi.required')
            .on('keyup blur', 'input', function () {
                validator.checkField.apply($(this).siblings().last()[0]);
        });
        // Input Validation End
    </script>
    @RenderSection("JavaScript", required: false)
</body>
</html>